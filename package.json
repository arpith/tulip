{
  "name": "tulip",
  "version": "0.0.2",
  "description": "Experimental Zulip app built using React",
  "main": "server.js",
  "scripts": {
    "lint": "eslint app index.js",
    "test": "npm run lint",
    "start": "parcel src/index.html",
    "build-client": "parcel build src/index.html -d dist/client --public-url /dist",
    "build-server": "babel src -d dist/server --ignore __tests__ --copy-files",
    "build": "npm run build-client && npm run build-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arpith/tulip.git"
  },
  "author": "Arpith Siromoney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arpith/tulip/issues"
  },
  "homepage": "https://github.com/arpith/tulip#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "^1.4.3",
    "emoji-js": "^3.4.0",
    "express": "^4.14.0",
    "moment": "^2.18.1",
    "react": "^15.3.1",
    "react-cookie": "^0.4.8",
    "react-dom": "^15.6.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "throng": "^4.0.0",
    "underscore": "^1.8.3",
    "zulip-js": "git+https://github.com/arpith/zulip-js.git",
    "parcel-bundler": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-assets-import-to-string": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1"
  },
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1"
  }
}
